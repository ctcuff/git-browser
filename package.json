{
  "name": "git-browser",
  "version": "1.0.0",
  "description": "A better way of viewing GitHub repositories in your browser",
  "author": "Cameron Cuff",
  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode='production' --env.debug='false'",
    "build:analyze": "yarn build --env.analyze",
    "check-types": "tsc --noemit",
    "dev": "webpack-dev-server --env.debug='true'",
    "dev:deploy": "yarn build && netlify deploy --open",
    "test": "jest --env=./test-env.js",
    "test:ci": "yarn test --bail --passWithNoTests --silent",
    "lint": "eslint './src/**/*{.js,.jsx,.ts,.tsx}'",
    "format": "yarn format:js && yarn format:scss",
    "format:js": "prettier --write './src/**/*{.js,.jsx,.ts,.tsx}'",
    "format:scss": "prettier --write './src/**/*.scss'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@google/model-viewer": "^1.3.0",
    "@octokit/rest": "^18.5.3",
    "@zip.js/zip.js": "^2.2.14",
    "anser": "^2.0.1",
    "asciidoctor": "^2.2.1",
    "dompurify": "^2.2.6",
    "firebase": "^8.2.1",
    "highlight.js": "^10.7.2",
    "lodash": "^4.17.20",
    "markdown-it": "^12.0.4",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-front-matter": "^0.2.3",
    "monaco-editor": "^0.21.2",
    "papaparse": "^5.3.0",
    "pdfjs-dist": "2.6.347",
    "psd.js": "^3.3.15",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-icons": "^4.1.0",
    "react-modal": "^3.12.1",
    "react-redux": "^7.2.2",
    "react-tabs": "^3.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "redux-watch": "^1.2.0",
    "simplebar-react": "^2.3.0",
    "urijs": "^1.19.4",
    "wabt": "^1.0.23",
    "@octokit/types": "^6.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/dompurify": "^2.2.2",
    "@types/lodash": "^4.14.169",
    "@types/markdown-it": "^12.0.1",
    "@types/papaparse": "^5.2.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-modal": "^3.12.0",
    "@types/react-tabs": "^2.3.2",
    "@types/urijs": "^1.19.15",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^6.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "lint-staged": "^10.5.3",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "node-sass": "^5.0.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.4",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "worker-plugin": "^5.0.0",
    "husky": "^6.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "defaults"
  ]
}
